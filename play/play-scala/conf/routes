# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.SimpleApplication.index
GET        /proxyBlocking       controllers.a_ws.BlockingProxyController.doProxy
GET        /proxy               controllers.a_ws.NonBlockingProxyController.doProxy
GET        /recoveredProxy      controllers.c_ws_recovery.NonBlockingRecoveredProxyController.doProxy

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)
